{
    "@idsType": "spr-bruker-sierra",
    "@idsVersion": "v1.0.0",
    "@idsNamespace": "common",
    "user": {
        "name": {
            "$path": "AssayData.UserName"
        }
    },
    "system": {
        "model": {
            "$path": "AssayData.InstrumentType"
        },
        "id": {
            "$path": "AssayData.SystemID"
        },
        "software": {
            "version": {
                "$path": "AssayData.ControlSoftwareVersion"
            }
        }
    },
    "experiment": {
        "assay": {
            "$path": "AssayData.AssayTitle"
        }
    },
    "method": {
        "id": {
            "$path": "AssayData.Method.UniqueID"
        },
        "instrument": {
            "surface_type": {
                "$path": "AssayData.SensorInformation.SurfaceType"
            }
        },
        "name": {
            "$path": "AssayData.Method.Name"
        },
        "description": {
            "$path": "AssayData.Method.Description"
        }
    },
    "run": {
        "log": [{
            "$each": "AssayData.DataEvents.DataEvent[*]",
            "name": "AssayData.DataEvents.DataEvent[*].Name",
            "even_type": "AssayData.DataEvents.DataEvent[*].EventType",
            "sample_time": "AssayData.DataEvents.DataEvent[*].SampleTime | _to_number",
            "real_time": "AssayData.DataEvents.DataEvent[*].RealTime",
            "time_index": "AssayData.DataEvents.DataEvent[*].TimeIndex | _to_int",
            "batch_id": "AssayData.DataEvents.DataEvent[*].BatchId",
            "message": "AssayData.DataEvents.DataEvent[*].Message"
        }]
    }
}